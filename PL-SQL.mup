      "attr": {
        "attachment": {
          "contentType": "text/html",
          "content": "<div>DECLARE</div><div>&nbsp; &nbsp; &nbsp; cursor cur_sailors is select * from sailors;</div><div>&nbsp; &nbsp; &nbsp; sailorData &nbsp; cur_sailors%ROWTYPE;</div><div>--Or</div><div>&nbsp; &nbsp; &nbsp; -- sailorData &nbsp; &nbsp; sailors%ROWTYPE;</div><div>&nbsp;&nbsp;</div><div>&nbsp;BEGIN</div><div>open cur_sailors; -- Memory is allocated to hold one row</div><div>LOOP</div><div>fetch cur_sailors into &nbsp;sailorData;</div><div>&nbsp;--Exactly one row is fetched by fetch statement</div><div><br></div><div>--Cursor Attribute (insert only first 5 characters into &nbsp;)</div><div>insert into temp_sailers values (substr (sailorData.name, 1,5))</div><div><br></div><div>EXIT WHEN cur_sailors%NOTFOUND;</div><div>--process the data&nbsp;</div><div>END LOOP;</div><div>-- in the end CURSORS must be closed</div><div><br></div><div>CLOSE cur_sailors; -- memory deallocation</div>"
