          "attr": {
            "attachment": {
              "contentType": "text/html",
              "content": "<span id=\"docs-internal-guid-346f0a48-115f-ed03-b623-26ea310370c6\"><p dir=\"ltr\" style=\"line-height:1.08;margin-top:0pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">DECLARE </font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">CURSOR wip_cur IS SELECT acct_no, enter_date FROM wip</font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><font size=\"3\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"> &nbsp;WHERE enter_date &lt; SYSDATE -7 </span><span style=\"font-family: Verdana; color: rgb(0, 32, 96); font-weight: 700; vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\">FOR UPDATE</span><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\">; </span></font></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">BEGIN </font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">FOR wip_rec IN wip_cur LOOP</font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\"> INSERT INTO acct_log (acct_no, order_date) VALUES (wip_rec.acct_no, wip_rec.enter_ date); </font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><font size=\"3\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\">DELETE FROM wip </span><span style=\"font-family: Verdana; color: rgb(0, 32, 96); font-weight: 700; vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\">WHERE CURRENT OF </span><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\">wip_cur;</span></font></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\"> END LOOP; </font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">COMMIT; -- releasing the lock</font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">ROLLBACK;-- releasing the lock</font></span></p><p dir=\"ltr\" style=\"line-height:1.08;margin-top:4.8pt;margin-bottom:0pt;margin-left: 21.6pt;text-indent: -21.6pt;\"><span style=\"font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><font size=\"3\">END; </font></span></p><div><span style=\"font-size: 32px; font-family: Verdana; color: rgb(0, 0, 0); vertical-align: baseline; white-space: pre-wrap; background-color: transparent;\"><br></span></div></span>"
