        "3": {
          "title": "parameterized Cursor",
          "id": 44,
          "attr": {
            "attachment": {
              "contentType": "text/html",
              "content": "<div>DECLARE</div><div>&nbsp;CURSOR cursorValue(p_product_id NUMBER) IS SELECT h.product_desc, o.company_short_name</div><div>&nbsp;FROM company o,product h</div><div>&nbsp;WHERE o.product_id =h.product_id</div><div>&nbsp;AND h.product_id =p_product_id ORDER by 2;</div><div>&nbsp;--v_company_rec cursorValue%ROWTYPE;</div><div>BEGIN</div><div>--OPEN cursorValue(1) ;</div><div>&nbsp;FOR idx in cursorValue(1) LOOP</div><div>&nbsp;dbms_output.put_line</div><div>(rpad(idx.product_desc,20,' ')||' '|| rpad(idx.company_short_name,30,' '));</div><div>&nbsp;END LOOP;</div><div>&nbsp;FOR idx in cursorValue(2) LOOP</div><div>&nbsp;dbms_output.put_line(rpad(idx.product_des,20,' ')||' '||</div><div>&nbsp;rpad(idx.company_short_name,30,' '));</div><div>&nbsp;END LOOP;</div><div>CLOSE cursor</div><div>END ;</div>"
            },
            "style": {},
            "position": [
              251.32991916162018,
              15.444477993765076,
              5
            ]
          }
